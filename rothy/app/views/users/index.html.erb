<h1>Listing users</h1>



<table>
  <tr>
		<th></th>
    <th>Vorname</th>
    <th>Nachname</th>
    <th>Email</th>
    <th>Username</th>
		<th>Semester</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
		<td><%= image_tag(user.avatar) %></td>
    <td><%= user.vorname %></td>
    <td><%= user.nachname %></td>
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.semester %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />


<h1>Listing users</h1>



<table>
  <tr>
    <th></th>
    <th>Vorname</th>
    <th>Nachname</th>
    <th>Email</th>
    <th>Username</th>
    <th>Semester</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @users.each do |user, userprofile| %>
      <tr>
        <td><%= image_tag(userprofile.avatar) %></td>
        <td><%= user.vorname %></td>
        <td><%= user.nachname %></td>
        <td><%= user.email %></td>
        <td><%= user.username %></td>
        <td><%= profile.semester %></td>
        <td>
          <%= link_to 'Show', user %>
          <br/>
          <%= link_to 'Edit', edit_user_path(user) %>
          <br/>
          <%= link_to 'Destroy', user, :confirm => 'Sind Sie sicher?', :method => :delete %>
        </td>
      </tr>
  <% end %>
</table>

